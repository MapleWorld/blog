<h1>Sign in</h1>

<div>
	<div>

		<%= form_for(:login, url: logins_path) do |f| %>

			<%= f.label :username %>
			<%= f.text_field :username %>

			<%= f.label :password %>
			<%= f.password_field :password %>

			<%= f.submit "Sign In" %>
		<% end %>

		<p>New user? <%= link_to "Sign up now!", new_user_path %></p>
	</div>


